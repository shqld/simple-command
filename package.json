    "license": "MIT",
    "scripts": {
        "test": "run-p test:*",
        "test:unit": "exit 0",
        "test:fmt": "prettier --check --loglevel warn .",
        "test:lint": "eslint src",
        "test:type": "tsc --noEmit",
